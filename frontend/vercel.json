{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "env": {
    "VITE_API_URL": "@api_url",
    "VITE_WS_URL": "@ws_url",
    "VITE_GOOGLE_CLIENT_ID": "@google_client_id"
  }
}